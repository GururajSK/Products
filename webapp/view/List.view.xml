<mvc:View controllerName="gsk.list.mylist.controller.List"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <content>
            <core:Fragment fragmentName="gsk.list.mylist.fragments.ListView" type="XML" />
         </content>
            <footer>
                <Toolbar id="idftoolbar">
                    <Button id="idNextBt" text="Next" press="onpressNext" />
                </Toolbar>
            </footer>
    </Page>
</mvc:View>
